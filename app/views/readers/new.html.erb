<h1> Sign up </h1>
<h5><%= flash[:error] %></h5>

<%= form_for @user do |f| %>
    <br>
    <%= f.label 'Username' %>
    <br>
    <%= f.text_field :username %>
    <br>
    <%= f.label 'password' %>
    <br>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit "Create Account" %>
<% end %>
<br>
<%= button_to "Log In", "/login", method: :get %>